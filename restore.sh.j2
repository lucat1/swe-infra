#!/bin/bash -x
cd /swe
docker-compose down
for dir in gitlab mattermost jenkins taiga sonarqube postgres swe
do
	rm -rf /$dir/*
	rsync -avzhp --numeric-ids -e "ssh -p {{ backup_port }}" root@{{ backup_host }}:/backup/$dir /
done
su postgres -c "psql -f /postgres/db.out postgres"
docker-compose up -d
